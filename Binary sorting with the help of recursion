def Search(elements,Element_to_search, min, max):
    if max >= min:
        mid = min + (max - min)//2
        if elements[mid] == Element_to_search:
            return mid
        elif elements[mid] > Element_to_search:
            return Search(elements, Element_to_search, min, mid-1)
        else:
            return Search(elements, Element_to_search, mid + 1, max)
    else:
        return -1
elements = [1,2,3, 4, 5, 6, 7, 8, 9]
Element_to_search=int(input("Enter element to be searched:"))
result = Search(elements, Element_to_search, 0, len(elements)-1)
if result != -1:
    print("Element found")
else:
    print("Not found")